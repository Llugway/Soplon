<!DOCTYPE html>
<html>
<%c++
	auto para=@@.get<std::unordered_map<std::string,std::string>>("parameters");
%>
<head>
	<meta charset="UTF-8">
	<title>[[ title ]]</title>
</head>
<body>
	<%c++ if(para.size()>0){%>
	<h1>Parameters</h1>
	<table border="1">
	<tr>
	  <th>name</th>
	  <th>value</th>
	</tr>
	<%c++ for(auto iter:para){%>
	<tr>
	  <td>{%iter.first%}</td>
	  <td><%c++ $$<<iter.second;%></td>
	</tr>
	<%c++}%>
	</table>
	<%c++ } else{%>
	<h1>no parameter</h1>
	<%c++}%>
</body>
</html>
